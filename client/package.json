{
  "name": "react-relay-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack --progress --colors --mode=production",
    "fetch-schema": "node scripts/fetch-schema.js",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md)\"",
    "install-or-update-required-vscode-extensions": "bash ./install-or-update-required-vscode-extensions.sh",
    "lint": "yarn type-check && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn type-check && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "patch-package",
    "relay": "relay-compiler",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --open --mode=development",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sorenhoyer/react-relay-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sorenhoyer/react-relay-boilerplate/issues"
  },
  "homepage": "https://github.com/sorenhoyer/react-relay-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@sorenhoyer/eslint-config-react": "^1.0.0",
    "@sorenhoyer/eslint-config-relay": "^1.0.0",
    "@sorenhoyer/prettier-config": "^1.0.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-relay": "^7.0.2",
    "@types/react-router": "^5.1.3",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.2",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^7.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-relay": "^1.4.1",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "graphql": "^14.5.8",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "relay-compiler": "^7.1.0",
    "relay-compiler-language-typescript": "^10.1.0",
    "relay-config": "^7.1.0",
    "typescript": "^3.7.2",
    "webpack": "^5.0.0-beta.7",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "oidc-client": "^1.9.1",
    "react": "^0.0.0-experimental-b53ea6ca0",
    "react-app-auth": "^1.2.0",
    "react-dom": "^0.0.0-experimental-b53ea6ca0",
    "react-relay": "^0.0.0-experimental-a1a40b68",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2"
  },
  "browsersList": [
    "Chrome >= 52",
    "FireFox >= 44",
    "Safari >= 9",
    "Explorer 11",
    "last 4 Edge versions"
  ],
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/typescript",
      "@babel/react"
    ],
    "plugins": [
      "relay",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@sorenhoyer/eslint-config-react",
      "@sorenhoyer/eslint-config-relay"
    ]
  },
  "eslintIgnore": [
    ".vscode",
    "coverage",
    "dist",
    "node_modules",
    "scripts",
    "webpack",
    "silent-renew",
    "__generated__"
  ],
  "prettier": "@sorenhoyer/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  }
}
